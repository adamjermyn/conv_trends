

&star_job

      create_pre_main_sequence_model = .true.
      pgstar_flag = .false.

/ ! end of star_job namelist

&eos
  ! eos options
  ! see eos/defaults/eos.defaults

/ ! end of eos namelist


&kap
  ! kap options
  ! see kap/defaults/kap.defaults
  use_Type2_opacities = .true.
  Zbase = 0.014

/ ! end of kap namelist

&controls

      initial_mass = MMM

      ! MAIN PARAMS
      mixing_length_alpha = 1.6
      initial_z = 0.014
      do_conv_premix = .true.
      use_Ledoux_criterion = .true.

      ! OUTPUT
      max_num_profile_models = 100000
      profile_interval = 10
      history_interval = 1

      ! WHEN TO STOP
      xa_central_lower_limit_species(1) = 'h1'
      xa_central_lower_limit(1) = 0.01

      ! RESOLUTION
      mesh_delta_coeff = 0.5
      time_delta_coeff = 1.0

      ! GOLD TOLERANCES
      use_gold_tolerances = .true.
      use_gold2_tolerances = .true.
      delta_lg_XH_cntr_limit = 0.01
      min_timestep_limit = 1d-1

      !limit on magnitude of relative change at any grid point
      delta_lgTeff_limit = 0.005
      delta_lgTeff_hard_limit = 0.005
      delta_lgL_limit = 0.005


      overshoot_scheme(1) = 'step'
      overshoot_zone_type(1) = 'any'
      overshoot_zone_loc(1) = 'any'
      overshoot_bdy_loc = 'any'
      overshoot_f(1) = 0.2d0
      overshoot_f0(1) = 0.1d0

/ ! end of controls namelist


&pgstar



/ ! end of pgstar namelist
